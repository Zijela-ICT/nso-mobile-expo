{
  "expo": {
    "name": "CHPRBN National Standing Order",
    "slug": "chprbn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "chprbn",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zijela.chprbn",
      "userInterfaceStyle": "light"
    },
    "android": {
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#ffffff"
      },
      "additionalRepositories": [
        "https://www.jitpack.io"
      ],
      "permissions": [],
      "package": "com.zijela.chprbn"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "react-native-video",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "950b1efd-4f89-4c82-bdb0-33c369492730"
      }
    }
  }
}
